[connection]
id={{ item.SSID }}
uuid={{ item.SSID | to_uuid }}
type=wifi
interface-name={{ item.interface | default('') }}
permissions= {% for i in item.permissions | default('', true) %}user:{{i}}:;{% endfor %}

[wifi]
mac-address-blacklist=
mode=infrastructure
ssid={{ item.SSID }}

[wifi-security]
auth-alg=open
key-mgmt=wpa-psk
psk={{ item.PSK }}

[ipv4]
dns-search=
method=auto

[ipv6]
addr-gen-mode=stable-privacy
dns-search=
method=auto

[proxy]